{
  "raw_dataset_path": "optData.csv",
  "clean_dataset_path": "cleaned_optData.csv",
  "plot_directory_path": "plots",
  "attributes": {
      "date": {
          "type": "nominal"
      },
      "act_symbol": {
          "type": "nominal"
      },
      "expiration": {
          "type": "nominal"
      },
      "strike": {
          "type": "real"
      },
      "call_put": {
          "type": "nominal"
      },
      "bid": {
          "type": "real"
      },
      "ask": {
          "type": "real"
      },
      "vol": {
          "type": "real"
      },
      "delta": {
          "type": "real"
      },
      "gamma": {
          "type": "real"
      },
      "theta": {
          "type": "real"
      },
      "vega": {
          "type": "real"
      },
      "rho": {
          "type": "real"
      }
  },
  "cleaning": [
      {
          "attribute": "date",
          "missing_strategy": "remove"
      },
      {
          "attribute": "act_symbol",
          "missing_strategy": "remove"
      },
      {
          "attribute": "expiration",
          "missing_strategy": "remove"
      },
      {
          "attribute": "strike",
          "missing_strategy": "remove"
      },
      {
          "attribute": "call_put",
          "missing_strategy": "remove"
      },
      {
          "attribute": "bid",
          "missing_strategy": "remove"
      },
      {
          "attribute": "ask",
          "missing_strategy": "remove"
      },
      {
          "attribute": "vol",
          "missing_strategy": "remove"
      },
      {
          "attribute": "delta",
          "missing_strategy": "replace_missing_with_mean"
      },
      {
          "attribute": "gamma",
          "missing_strategy": "replace_missing_with_mean"
      },
      {
          "attribute": "theta",
          "missing_strategy": "replace_missing_with_mean"
      },
      {
          "attribute": "vega",
          "missing_strategy": "replace_missing_with_mean"
      },
      {
          "attribute": "rho",
          "missing_strategy": "replace_missing_with_mean"
      }
  ],
  "transforming": [
         {
          "action": "split_date",
          "attribute": "date",
          "args": [],
          "kwargs": {
              "date_format": "%Y-%m-%d"
          }
      },
      {
        "attribute": "date_year",
        "action": "filter_by_year_and_month",
        "args": [[2020], [1, 2, 3, 4, 5, 6]],  
        "kwargs": {}
    }
  ],
  "plotting": [
      {
          "action": "make_density_plot",
          "attribute": "strike",
          "name": "StrikeDensity"
      },
      {
          "action": "make_density_plot",
          "attribute": "vol",
          "name": "VolDensity"
      }
  ]
}